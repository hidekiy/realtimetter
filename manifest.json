{
	"manifest_version": 2,
	"name": "Realtimetter",
	"description": "__MSG_appDesc__",
	"version": "0.7.1",
	"default_locale": "en",
	"app": {
		"launch": {
			"local_path": "index.html"
		}
	},
	"icons": {
		"16": "icon_16.png",
		"128": "icon_128.png"
	},
	"permissions": [
		"notifications",
		"https://*.twitter.com/"
	],
	"web_accessible_resources": [
		"icon_128.png",
		"index.html"
	]
}
